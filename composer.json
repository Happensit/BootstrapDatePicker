{
    "name": "happensit/bootstrap-date-picker-widget",
    "description":"Date/time picker widget based on Twitter bootstrap with automatic format timestamp",
    "homepage": "https://github.com/happensit",
    "type": "yii2-extension",
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/Happensit/BootstrapDatePicker"
      }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "bower-asset/eonasdan-bootstrap-datetimepicker": "^4.7@dev"
    },
    "authors": [
        {
            "name": "Happensit",
            "email": "antonybizov@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
      "psr-4": {
        "happensit\\datepicker\\": "datepicker"
      }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
